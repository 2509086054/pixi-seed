{
  "name": "pixi-seed",
  "version": "0.3.0",
  "repository": "https://github.com/edwinwebb/pixi-seed",
  "description": "Pixi.js project seed / boiler-plate with ES6, Webpack and Redux",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server",
    "webpack": "webpack -p --config ./webpack.config.js",
    "build": "NODE_ENV=production npm run webpack",
    "prewebpack": "npm run clean",
    "clean": "rimraf ./build && mkdir ./build",
    "lint": "eslint ./app/**/*.js",
    "reactman": "reactman --config ./templates/reactman.config.js"
  },
  "dependencies": {
    "pixi.js": "^3.0.7",
    "redux": "^3.7.2",
    "tween.js": "^16.3.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^2.4.0",
    "file-loader": "^0.8.4",
    "html-loader": "^0.4.3",
    "json-loader": "^0.5.3",
    "reactman": "^3.2.0",
    "rimraf": "^2.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
